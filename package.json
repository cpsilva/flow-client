{
  "name": "flow-client-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "config": "tsx ./src/scripts/setEnv.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=prod && ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "ng update --force @angular/cli @angular/core",
    "store-version": "echo '{\"version\" : '$(cat package.json | jq .version )'}' > public/version.json && git add public/version.json && git commit -m \"version upgrade \"",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint && lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "17.2.1",
    "@angular/cdk": "^17.3.5",
    "@angular/common": "17.2.1",
    "@angular/compiler": "17.2.1",
    "@angular/core": "17.2.1",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "17.2.1",
    "@angular/localize": "17.2.1",
    "@angular/platform-browser": "17.2.1",
    "@angular/platform-browser-dynamic": "17.2.1",
    "@angular/router": "17.2.1",
    "@igniteui/material-icons-extended": "^3.0.0",
    "@ng-bootstrap/ng-bootstrap": "^14.1.1",
    "@ngrx/store": "17.1.0",
    "@profabric/angular-components": "0.2.4",
    "@types/hammerjs": "^2.0.39",
    "bootstrap": "5.3.2",
    "bpmn-js": "^17.0.1",
    "core-js": "2.6.2",
    "fflate": "^0.8.1",
    "hammerjs": "^2.0.8",
    "igniteui-angular": "^17.1.9",
    "igniteui-angular-core": "^17.3.0",
    "immediate": "^3.2.3",
    "intl": "^1.2.5",
    "lit": "3.1.2",
    "luxon": "3.4.4",
    "minireset.css": "~0.0.4",
    "ngx-toastr": "18.0.0",
    "process": "0.11.10",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "uuid": "9.0.1",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "17.2.0",
    "@angular/compiler-cli": "17.2.1",
    "@angular/language-service": "17.2.1",
    "@igniteui/angular-schematics": "~17.1.1316",
    "@types/hammerjs": "^2.0.40",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "2.0.13",
    "@types/luxon": "^3.4.2",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "admin-lte": "3.2.0",
    "dotenv": "16.4.4",
    "eslint": "8.56.0",
    "fs-web": "1.0.1",
    "husky": "9.0.11",
    "jasmine-core": "~5.1.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.2.2",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "prettier": "3.2.5",
    "pretty-quick": "4.0.0",
    "protractor": "7.0.0",
    "standard-version": "9.5.0",
    "ts-node": "10.9.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "tsx": "^4.7.1",
    "typescript": "5.3.3",
    "yargs": "17.7.2"
  }
}
